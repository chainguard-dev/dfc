FROM debian:11

# Multiple package manager commands
RUN apt-get update && \
    apt-get install -y curl ca-certificates git && \
    apt-get clean && \
    apt-get autoremove -y && \
    apt-cache gencaches

# Mix of package managers
RUN apt update && \
    apt install -y nginx python3 python3-pip && \
    apt clean

# Some other commands
WORKDIR /app
COPY . .
RUN echo "test"

CMD ["bash"] 